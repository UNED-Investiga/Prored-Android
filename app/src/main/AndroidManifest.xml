<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nansoft.prored" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.RedActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activity.PerfilUsuarioActivity"
            android:label="@string/title_activity_perfil_usuario" >
        </activity>
        <activity
            android:name=".Activity.EventoActivity"
            android:label="@string/title_activity_evento" >
        </activity>
        <activity
            android:name=".Activity.InfoEventoActivity"
            android:label="@string/title_activity_info_evento" >
        </activity>
    </application>

</manifest>
